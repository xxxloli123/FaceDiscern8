<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.android.face_discern"
          xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- camera -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- sdcard -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <!-- wifi internet -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.android.face_discern.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name="com.android.face_discern.PermissionAcitivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.android.face_discern.RegisterActivity">
        </activity>
        <activity android:name="com.android.face_discern.DetecterActivity"
            android:theme="@android:style/Theme.NoTitleBar"
            >
        </activity>
        <activity android:name="com.android.face_discern.MainActivity">
        </activity>
    </application>

</manifest>